{
  "name": "acmetemplate/laravel-translation-manager",
  "description": "Manage Laravel Translations",
  "keywords": [
    "laravel",
    "translations",
    "translator"
  ],
  "license": "MIT",
  "authors": [
    {
        "name": "Acme Saico",
        "email": "info@acmesaico.com",
        "homepage": "https://acmesaico.com"
    }
  ],
  "require": {
    "php": ">=7",
    "illuminate/support": "*",
    "illuminate/translation": "*",
	"mcamara/laravel-localization": "*",
    "symfony/finder": "*",
    "tanmuhittin/laravel-google-translate": "*",
  },
  "autoload": {
    "psr-4": {
      "Acmetemplate\\TranslationManager\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5-dev"
    },
    "laravel": {
      "providers": [
        "Acmetemplate\\TranslationManager\\ManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
